<%- include('./partials/header.ejs') %>
<%- include('./partials/navbar.ejs') %>

<div style="text-align: center">
   <table border align="center" width="100%">
      <tr>
         <th>Username</th>
         <th>Email</th>
         <th>Phone Number</th>
         <th>Games</th>
      </tr>
      <% data.forEach(user => { %>
         <tr>
            <td><%= user.username %></td>
            <td><%= user.email %></td>
            <td><%= user.phone_number %></td>
            <td>
               <% gamesInUser.forEach((el, i) => { %>
                  <% if(Object.keys(el)[0] === user.username) { %>
                     <%= gamesInUser[i][user.username] %>
                  <% } %>
               <% }) %>
            </td>
         </tr>
      <% }) %>
   </table>
</div>




<%- include('./partials/footer.ejs') %>